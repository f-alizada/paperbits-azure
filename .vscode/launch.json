    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Test storage",
            "type": "node",
            "request": "launch",
            "sourceMaps": true,
            "protocol": "inspector",
            "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
            "args": [
                "-r",
                "ts-node/register",
                "${workspaceRoot}/specs/azureBlobStorage.spec.ts"
            ],
            "stopOnEntry": false,
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            "console": "internalConsole"
        },
    ]
}